{
  "routes": [
    {
      "route": "/path1",
      "redirect": "/",
      "statusCode": 301
    },
    {
      "route": "/test",
      "redirect": "/"
    },
    {
      "route": "/test2",
      "rewrite": "/"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".html": "text/html"
  }
}